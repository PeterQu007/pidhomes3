<div>
  {# {{ dump(map_info) }} #}
</div>
<hr class="pid_separator">
<section class="rh_section rh_section--flex rh_wrap--padding rh_wrap--topPadding pid_wrap--padding">
  <div class="rh_page rh_page__listing_page rh_page__main">
    {# RENDER MAP #}
    {{ include('partials-twig/components/pid-map.twig', map_info) }}
    {# PAGE TITLE #}
    {% if is_market_page %}
      <h1 class="pid_community_h1">
      <i class="fas fa-chart-line" aria-hidden="true"> </i> 
      {{ market_section_h1 }}</h1>
    {% endif %}
    {# RENDER SOCIAL SHARE BUTTONS #}
    {{ include('partials-twig/components/social-share.twig', social_share) }}

    {# MARKET METABOX BAR #}
    <section>
      <div>
        {# <img class="pid_section_leading_icon" src={{section_icon}}> #}
        <div class="pid_section_leading_icon">
          <i class="far fa-question-circle" style="font-size:3.5rem"></i>        
        </div>
        <p class="pid_section_content">{{section_content}}</p>
      </div>
      <div class="pid_section_wrapper">
        {% if not is_market_page %}
          <h1 class="pid_community_h1">
          <i class="fas fa-chart-line" aria-hidden="true"> </i> 
          {{ market_section_h1 }}</h1>
        {% endif %}
        {{ include ('partials-twig/components/market-stats.twig', market) }}
      </div>
    </section>

    {# RENDER HPI TABLES #}
    <section>
      <h2 class="pid_separator pid_h2">{{HPI_table_title}}</h2>

      {% if community_level == "0" %}
        {% filter shortcodes %}
          [wpdatatable id=29]
        {% endfilter %}
        {% filter shortcodes %}
          [wpdatachart id=2]
        {% endfilter %}
      {% else %}
        {% filter shortcodes %}
          [wpdatatable id=45 VAR1={{nbh_codes}}]
        {% endfilter %}
      {% endif %}

    </section>

    {# RENDER RPS LISTING SHOWCASE #}
    {{ include('partials-twig/components/rps-listings.twig', rps_listings) }}

    {# RENDER MORE COMMUNITY MARKET CHARTS LINKS #}
    {{ include('partials-twig/components/related-communities.twig', related_communities)}}

  </div>

  {# CREATE PAGE SIDEBAR #}
  <div class="rh_page rh_page_sidebar">
    {{ function('get_sidebar','default') }}
  </div>
</section>

{# REDNER PAGE FOOTER #}
{{ function('get_template_part', 'assets/modern/partials/banner/peterqu') }}
{{ function('get_footer') }}
